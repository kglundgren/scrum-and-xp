@using scrum_and_xp.ViewModels
@model List<UserRoleViewModel>
@{
    ViewBag.Title = "NewUsers";
  
}

<h2>NewUsers</h2>

<table class="table">
    <tr>
        <th>
            Role Name
        </th>
        <th>
            Role Id
        </th>
    </tr>
    @if (Model.Any())
    {
        foreach (var role in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => role.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => role.UserId)
                </td>
                <td>
                </td>
            </tr>
        }
    }

</table>